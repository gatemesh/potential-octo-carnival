{
  "name": "gatemesh-web-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter web build",
    "build:check": "pnpm --filter web build:check",
    "preview": "pnpm --filter web preview",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "clean": "pnpm -r exec rm -rf dist build node_modules/.cache",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "verify:build": "node scripts/verify-build.js",
    "postbuild": "pnpm verify:build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}